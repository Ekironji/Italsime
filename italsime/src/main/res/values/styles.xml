<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#fafafa</item>
        <item name="colorPrimaryDark">#616161</item>
        <item name="colorAccent">#9e9e9e</item>
    </style>

    <!-- Application theme. -->
    <style name="ToolbarCustomIconColor" parent="AppTheme">
        <!-- android:textColorSecondary is used for coloring the navigation drawer icon and up arrow. -->
        <item name="android:textColorSecondary">#616161</item>
    </style>

</resources>
